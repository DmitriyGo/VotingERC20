{
  "name": "voting-token",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "build": "yarn clean && yarn compile",
    "test": "yarn build && hardhat test",
    "coverage": "cross-env COVERAGE=true hardhat coverage --solcoverjs ./.solcover.json",
    "node:start": "hardhat node --no-deploy",
    "node:deploy:gmx": "hardhat deploy --network localhost",
    "lint:ts": "eslint \"**/*.{js,ts}\"",
    "lint:ts:fix": "eslint \"**/*.{js,ts}\" --fix",
    "lint:sol": "solhint \"contracts/**/*.sol\"",
    "lint:sol:fix": "solhint \"contracts/**/*.sol\" --quiet --fix",
    "format:ts": "prettier \"**/*.{js,ts,json}\" --check",
    "format:ts:fix": "prettier \"**/*.{js,ts,json}\" --write",
    "format:sol": "prettier \"contracts/**/*.sol\" --check",
    "format:sol:fix": "prettier \"contracts/**/*.sol\" --write",
    "format:yml": "prettier \"**/*.{yml,yaml}\" --check",
    "format:yml:fix": "prettier \"**/*.{yml,yaml}\" --write",
    "codestyle": "run-s lint:* format:*",
    "codestyle:fix": "run-s lint:*:* format:*:*"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=16.0.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "chai": "^4.2.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^6.4.0",
    "hardhat": "^2.20.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.12.1",
    "hardhat-gas-reporter": "^1.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^4.1.1",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2"
  }
}
